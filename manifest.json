{
    "name": "Tmail For Podio",
    "description": "Tmail Extension for Podio",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches":[
          "https://podio.com/*",
			    "https://*.podio.com/*"
        ],
        "js":["jquery-3.6.0.min.js", "app.js"]
      }
    ],
    "icons": { 
      "16": "images/tmail.png",
      "32": "images/tmail2.png",
      "48": "images/tmail3.png",
     "128": "images/tmail4.png" 
    }
}